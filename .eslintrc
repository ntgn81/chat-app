{
  "extends": ["react", "prettier"],
  "plugins": ["react", "prettier"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "jsxBracketSameLine": true
      }
    ]
  }
}
